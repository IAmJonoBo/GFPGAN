   "source": [
    "# GFPGAN Colab Demo (Fork)\n",
    "\n",
    "This Colab notebook installs dependencies, clones this fork, downloads pretrained weights automatically, and runs the GFPGAN inference script on sample images."
   ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "ui-panel"
      },
      "source": [
        "#@title Interactive UI (upload, run, preview, download)\n",
        "from ipywidgets import VBox, HBox, Button, IntSlider, FloatSlider, Dropdown, Checkbox, Output, FileUpload, Label, Textarea\n",
        "from IPython.display import display, clear_output, Image as IPyImage\n",
        "import os\n",
        "import shutil\n",
        "import subprocess\n",
        "import glob\n",
        "\n",
        "repo_dir = 'GFPGAN'\n",
        "uploads_dir = os.path.join(repo_dir, 'uploads')\n",
        "results_dir = os.path.join(repo_dir, 'results')\n",
        "os.makedirs(uploads_dir, exist_ok=True)\n",
        "\n",
        "# Controls\n",
        "ver = Dropdown(options=['1', '1.2', '1.3', '1.4', 'RestoreFormer'], value='1.4', description='Version:')\n",
        "scale = IntSlider(value=2, min=1, max=4, step=1, description='Upscale:')\n",
        "weight = FloatSlider(value=0.5, min=0.0, max=1.0, step=0.05, description='Weight:')\n",
        "only_center = Checkbox(value=False, description='Only center face')\n",
        "aligned = Checkbox(value=False, description='Aligned input')\n",
        "bg_label = Label(value='Background upsampler: Real-ESRGAN (auto CPU/GPU)')\n",
        "uploader = FileUpload(accept='image/*', multiple=True, description='Upload images')\n",
        "url_box = Textarea(placeholder='One URL per line', description='URLs:', layout=dict(width='100%', height='80px'))\n",
        "fetch_btn = Button(description='Fetch URLs')\n",
        "run_btn = Button(description='Run GFPGAN', button_style='primary')\n",
        "clear_btn = Button(description='Clear results')\n",
        "zip_btn = Button(description='Download ZIP')\n",
        "out = Output()\n",
        "\n",
        "def _save_uploads():\n",
        "    # ipywidgets 7/8 compatibility\n",
        "    files = uploader.value\n",
        "    if isinstance(files, dict):\n",
        "        items = files.items()\n",
        "    else:\n",
        "        items = [(f['name'], f) for f in files]\n",
        "    saved = []\n",
        "    for name, f in items:\n",
        "        content = f.get('content') if isinstance(f, dict) else f['content']\n",
        "        dest = os.path.join(uploads_dir, os.path.basename(name))\n",
        "        with open(dest, 'wb') as fp:\n",
        "            fp.write(content)\n",
        "        saved.append(dest)\n",
        "    return saved\n",
        "\n",
        "def _fetch_urls():\n",
        "    import hashlib, pathlib\n",
        "    import requests\n",
        "    saved = []\n",
        "    text = (url_box.value or '').strip()\n",
        "    if not text:\n",
        "        return saved\n",
        "    for line in text.splitlines():\n",
        "        url = line.strip()\n",
        "        if not url:\n",
        "            continue\n",
        "        try:\n",
        "            r = requests.get(url, timeout=20)\n",
        "            r.raise_for_status()\n",
        "            ext = pathlib.Path(url).suffix or '.jpg'\n",
        "            name = hashlib.sha1(url.encode('utf-8')).hexdigest() + ext\n",
        "            dest = os.path.join(uploads_dir, name)\n",
        "            with open(dest, 'wb') as f:\n",
        "                f.write(r.content)\n",
        "            saved.append(dest)\n",
        "        except Exception as e:\n",
        "            print('Failed to fetch', url, e)\n",
        "    return saved\n",
        "\n",
        "def _preview(limit=6):\n",
        "    imgs = sorted(glob.glob(os.path.join(results_dir, 'restored_imgs', '*')))[:limit]\n",
        "    if not imgs:\n",
        "        print('No restored images found yet.')\n",
        "        return\n",
        "    # Grid preview\n",
        "    for p in imgs:\n",
        "        display(IPyImage(filename=p))\n",
        "    # Simple before/after slider for first image if original found\n",
        "    try:\n",
        "        first = imgs[0]\n",
        "        import os as _os, glob as _glob, html as _html\n",
        "        b = _os.path.basename(first)\n",
        "        stem = _os.path.splitext(b)[0]\n",
        "        # try uploads folder for original\n",
        "        cand = _glob.glob(_os.path.join(uploads_dir, stem) + '.*')\n",
        "        if cand:\n",
        "            orig = cand[0]\n",
        "            html = f'''\n",
        "<style>\n",
        ".cmp-wrap{{position:relative;max-width:720px;}}\n",
        ".cmp-wrap img{{display:block;width:100%;}}\n",
        ".cmp-top{{position:absolute;left:0;top:0;overflow:hidden;width:50%;}}\n",
        ".cmp-slider{{width:100%;}}\n",
        "</style>\n",
        "<div class=\\"cmp-wrap\\">\n",
        f"  <img src=\\"{orig}\\"/>\n",
        "  <div class=\\"cmp-top\\" id=\\"cmp-top\\">\n",
        f"    <img src=\\"{first}\\"/>\n",
        "  </div>\n",
        "</div>\n",
        "<input type=\\"range\\" min=\\"0\\" max=\\"100\\" value=\\"50\\" class=\\"cmp-slider\\" oninput=\\"document.getElementById('cmp-top').style.width=this.value+'%';\\">\n",
        "'''\n",
        "            from IPython.display import HTML as _HTML\n",
        "            display(_HTML(html))\n",
        "    except Exception as e:\n",
        "        print('Slider preview unavailable:', e)\n",
        "\n",
        "def on_run_clicked(_):\n",
        "    out.clear_output()\n",
        "    with out:\n",
        "        os.makedirs(results_dir, exist_ok=True)\n",
        "        saved = _save_uploads()\n",
        "        saved += _fetch_urls()\n",
        "        if not saved:\n",
        "            print('Please upload at least one image.')\n",
        "            return\n",
        "        cmd = ['python', 'inference_gfpgan.py', '-i', 'uploads', '-o', 'results', '-v', str(ver.value), '-s', str(scale.value), '--bg_upsampler', 'realesrgan']\n",
        "        if only_center.value:\n",
        "            cmd.append('--only_center_face')\n",
        "        if aligned.value:\n",
        "            cmd.append('--aligned')\n",
        "        cmd.extend(['-w', str(weight.value)])\n",
        "        print('Running:', ' '.join(cmd))\n",
        "        proc = subprocess.run(cmd, cwd=repo_dir, capture_output=True, text=True)\n",
        "        print(proc.stdout)\n",
        "        if proc.returncode != 0:\n",
        "            print('Error:\n', proc.stderr)\n",
        "        _preview()\n",
        "\n",
        "def on_clear_clicked(_):\n",
        "    out.clear_output()\n",
        "    if os.path.isdir(results_dir):\n",
        "        shutil.rmtree(results_dir)\n",
        "    os.makedirs(results_dir, exist_ok=True)\n",
        "    with out:\n",
        "        print('Cleared results directory.')\n",
        "\n",
        "def on_zip_clicked(_):\n",
        "    from google.colab import files as colab_files\n",
        "    out.clear_output()\n",
        "    zip_path = os.path.join(repo_dir, 'results.zip')\n",
        "    if os.path.exists(zip_path):\n",
        "        os.remove(zip_path)\n",
        "    # zip results folder\n",
        "    !cd GFPGAN && zip -qr results.zip results || true\n",
        "    with out:\n",
        "        if os.path.exists(zip_path):\n",
        "            print('Downloading', zip_path)\n",
        "            colab_files.download(zip_path)\n",
        "        else:\n",
        "            print('No results to download yet.')\n",
        "\n",
        "run_btn.on_click(on_run_clicked)\n",
        "fetch_btn.on_click(lambda _: (_fetch_urls(), print('Fetched URLs (if valid).')) )\n",
        "clear_btn.on_click(on_clear_clicked)\n",
        "zip_btn.on_click(on_zip_clicked)\n",
        "\n",
        "controls = VBox([HBox([ver, scale]), HBox([weight]), HBox([only_center, aligned]), bg_label, uploader, HBox([url_box]), HBox([fetch_btn, run_btn, clear_btn, zip_btn])])\n",
        "display(VBox([controls, out]))\n"
      ],
      "execution_count": null,
      "outputs": []
    },
